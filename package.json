    "@pdftron/webviewer": "^8.3.3",
    "core-js": "^3.17.3",
    "nuxt": "^2.14.12",
    "nuxt-mail": "^3.0.23",
    "vee-validate": "^4.4.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^13.1.0",
    "@nuxt/types": "^2.16.0",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
