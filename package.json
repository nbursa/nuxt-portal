    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^7.5.0",
    "fiber": "^1.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.1.0",
    "sass": "^1.58.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylus": "^0.59.0",
